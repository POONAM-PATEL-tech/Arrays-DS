void print(int ar[], int n)
{
    vector<int> a;
    for(int i=0;i<n;i++){
        if(i%2==0){
        a.push_back(ar[i]);
            
        }
        
        
    }
    // code here
    for(int i=0;i<a.size();i++){
        cout<<a[i]<<" ";
    }
    
    
}
int main(){
int t;
cin>>t;
while(t--){
int ar[1000001]={0};
int n;
cin>>n;
for(int i=0;i<n;i++){
cin>>arr[i];
}
print(ar,n);
cout<<endl;
}

return 0;
}
